{
  "name": "directorial",
  "version": "0.0.0",
  "description": "Shell Executor",
  "main": "index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:InteractiveTimmy/directorial.git",
  "author": "Timothy Scheuering",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:bundle": "esbuild ./src/index.ts --bundle --format=esm --minify --outfile=./build/bundle.js",
    "build:bundle:clean": "rimraf ./build",
    "build:clean": "yarn build:bundle:clean",
    "build:dist": "tsc",
    "build:dist:clean": "rimraf ./dist",
    "build:dist:validate": "tsc --no-emit",
    "build:docs": "echo \"command is not yet implemented\"",
    "lint": "eslint ./src/**/*.*",
    "lint:fix": "yarn lint --fix",
    "start": "echo \"command is not yet implemented\"",
    "test": "yarn test:unit && yarn test:integration",
    "test:integration": "jest \"src/.*\\.integration\\.test\\.js\"",
    "test:unit": "jest \"src/.*\\.unit\\.test\\.js\""
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "esbuild": "^0.12.15",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "4.3.8",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  }
}
